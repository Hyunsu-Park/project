-- 테이블 health_db.product_info 구조 내보내기
CREATE TABLE IF NOT EXISTS `product_info` (
  `pi_seq` int NOT NULL AUTO_INCREMENT,
  `pi_name` varchar(50) CHARACTER SET utf8 COLLATE utf8_general_ci NOT NULL COMMENT '상품이름',
  `pi_price` varchar(50) CHARACTER SET utf8 COLLATE utf8_general_ci NOT NULL COMMENT '상품가격',
  `pi_seller` varchar(50) CHARACTER SET utf8 COLLATE utf8_general_ci NOT NULL COMMENT '상품판매자',
  `pi_reg_date` datetime NOT NULL DEFAULT CURRENT_TIMESTAMP COMMENT '상품등록일',
  `pi_introduce` varchar(50) CHARACTER SET utf8 COLLATE utf8_general_ci NOT NULL COMMENT '상품설명',
  `pi_category_seq` int NOT NULL COMMENT '상품분류(현재 1식품 2보호구 3회원권 4기구 5운동복)',
  `pi_img` varchar(50) CHARACTER SET utf8 COLLATE utf8_general_ci NOT NULL DEFAULT '/resources/images/이미지없음.jpg',
  PRIMARY KEY (`pi_seq`)
) ENGINE=InnoDB AUTO_INCREMENT=29 DEFAULT CHARSET=utf8 COMMENT='상품정보';

