-- 테이블 health_db.user_volume 구조 내보내기
CREATE TABLE IF NOT EXISTS `user_volume` (
  `uv_seq` int NOT NULL AUTO_INCREMENT,
  `uv_exercise_name` varchar(50) CHARACTER SET utf8 COLLATE utf8_general_ci NOT NULL COMMENT '종목이름',
  `uv_date` datetime NOT NULL DEFAULT CURRENT_TIMESTAMP COMMENT '일별',
  `uv_weight` int NOT NULL DEFAULT '0' COMMENT '중량',
  `uv_amount` int NOT NULL DEFAULT '0' COMMENT '갯수',
  `uv_bt` int NOT NULL DEFAULT '60' COMMENT 'breaktime : 쉬는시간',
  `uv_category_seq` int NOT NULL COMMENT '카테고리 번호',
  `uv_id` varchar(50) CHARACTER SET utf8 COLLATE utf8_general_ci NOT NULL COMMENT '유저 id',
  PRIMARY KEY (`uv_seq`)
) ENGINE=InnoDB AUTO_INCREMENT=37 DEFAULT CHARSET=utf8 COMMENT='회원의 일별 운동량 기록';

