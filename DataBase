-- 테이블 health_db.qna_post 구조 내보내기
CREATE TABLE IF NOT EXISTS `qna_post` (
  `qp_seq` int NOT NULL AUTO_INCREMENT,
  `qp_title` varchar(10000) CHARACTER SET utf8 COLLATE utf8_general_ci NOT NULL,
  `qp_content` varchar(10000) CHARACTER SET utf8 COLLATE utf8_general_ci NOT NULL,
  `qp_owner` varchar(50) CHARACTER SET utf8 COLLATE utf8_general_ci NOT NULL,
  `qp_board_seq` int NOT NULL COMMENT '0:질문(question)//1:요청(request)//2:신고(report)',
  `qp_reg_date` datetime NOT NULL DEFAULT CURRENT_TIMESTAMP,
  `qp_answer` int NOT NULL DEFAULT '0' COMMENT '0: 답없음 // 1:답있음',
  PRIMARY KEY (`qp_seq`)
) ENGINE=InnoDB AUTO_INCREMENT=74 DEFAULT CHARSET=utf8 COMMENT='문의게시판';

-- 테이블 데이터 health_db.qna_post:~73 rows (대략적) 내보내기
