-- 테이블 health_db.user_info 구조 내보내기
CREATE TABLE IF NOT EXISTS `user_info` (
  `ui_seq` int NOT NULL AUTO_INCREMENT,
  `ui_id` varchar(10) CHARACTER SET utf8 COLLATE utf8_general_ci NOT NULL,
  `ui_pwd` varchar(20) CHARACTER SET utf8 COLLATE utf8_general_ci NOT NULL,
  `ui_name` varchar(50) NOT NULL,
  `ui_age` int NOT NULL,
  `ui_addr` varchar(100) CHARACTER SET utf8 COLLATE utf8_general_ci NOT NULL,
  `ui_weight` int NOT NULL,
  `ui_height` int NOT NULL,
  `ui_reg_date` datetime NOT NULL DEFAULT CURRENT_TIMESTAMP,
  `ui_type` int NOT NULL DEFAULT '0' COMMENT '0:일반회원 / 1:우수회원 / 2:관리자',
  PRIMARY KEY (`ui_seq`)
) ENGINE=InnoDB AUTO_INCREMENT=30 DEFAULT CHARSET=utf8;

